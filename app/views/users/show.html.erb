<% provide :title, @user.name %>
<div class="container">
  <div class="col-md-3">
    <%= render "users/user_board"%>
  </div>
  <div class="col-md-9">
    <div class="user-info">
      <h3><%= t ".info" %></h3>
      <p><%= t ".name", name: @user.name %></p>
      <p><%= t ".email", email: @user.email %></p>
      <p><%= t ".phone", phone: @user.phone %></p>
    </div>
  </div>
</div>
