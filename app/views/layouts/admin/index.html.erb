<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title yield(:title) %></title>
    <%= stylesheet_link_tag "admin/style", media: "all",
      "data-turbolinks-track": true %>
    <%= javascript_include_tag "admin/main", "data-turbolinks-track": true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="wrapper">
     <%= render "layouts/admin/header" %>
     <div id="page-wrapper">
        <div class="row">
          <div class="col-lg-12">
            <%= render "shared/flash" %>
            <%= yield %>
            </div>
          </div>
        </div>
    </div>
  </body>
</html>
